<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 <hibernate-mapping>  
  <class name="com.LocationCrud.LocationEntity" table="LOCATIONS">  
    <id name="locId" column="LOCATION_ID">  
    <generator class="sequence">
        <param name="sequence">LOCATIONS_SEQ</param>
     </generator> 
    </id>         
    <property name="streetAdd" column="STREET_ADDRESS"></property> 
    <property name="postalCode" column="POSTAL_CODE"></property>
    <property name="city" column="CITY"></property>        
    <property name="stateProvince" column="STATE_PROVINCE"></property>        
    <property name="countryId" column="COUNTRY_ID"></property>        
  </class>  
 </hibernate-mapping>
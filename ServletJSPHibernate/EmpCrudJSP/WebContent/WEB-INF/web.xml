<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <welcome-file-list>
    <welcome-file>/views/login.jsp</welcome-file>
  </welcome-file-list>
    <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.EmpCrudJSP.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginFail</servlet-name>
    <jsp-file>/views/login.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>EmpDtlPage</servlet-name>
    <servlet-class>com.EmpCrudJSP.EmpDtlPageServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetEmpDetail</servlet-name>
    <servlet-class>com.EmpCrudJSP.GetEmpDetailServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EmpDetailPage</servlet-name>
    <jsp-file>/views/empdetail.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>HomeServlet</servlet-name>
    <jsp-file>/views/home.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>UpdateEmpDtlPage</servlet-name>
    <servlet-class>com.EmpCrudJSP.UpdateEmpDetailServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/loginServlet</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>LoginFail</servlet-name>
    <url-pattern>/loginFail</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EmpDtlPage</servlet-name>
    <url-pattern>/getEmployeeDetailPage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetEmpDetail</servlet-name>
    <url-pattern>/getEmpDetail</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EmpDetailPage</servlet-name>
    <url-pattern>/empdetail</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>HomeServlet</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>UpdateEmpDtlPage</servlet-name>
    <url-pattern>/updateEmployeeDetailPage</url-pattern>
  </servlet-mapping>
  </web-app>
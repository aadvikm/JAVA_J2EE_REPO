<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <welcome-file-list>
    <welcome-file>/views/index.html</welcome-file>
  </welcome-file-list>
   <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.emp.crud.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EmpDtlPage</servlet-name>
    <servlet-class>com.emp.crud.EmpDtlPageServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetEmpDetail</servlet-name>
    <servlet-class>com.emp.crud.GetEmpDetailServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>HomeServlet</servlet-name>
    <servlet-class>com.emp.crud.HomeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UpdateEmpDtlPage</servlet-name>
    <servlet-class>com.emp.crud.UpdateEmpDetailServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EmpDtlPage</servlet-name>
    <url-pattern>/getEmployeeDetailPage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetEmpDetail</servlet-name>
    <url-pattern>/getEmpDetail</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>HomeServlet</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>UpdateEmpDtlPage</servlet-name>
    <url-pattern>/updateEmployeeDetailPage</url-pattern>
  </servlet-mapping>
  
  
  
   
</web-app>